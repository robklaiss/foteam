{
    "name": "foteam/photography-platform",
    "description": "A platform for managing and selling marathon photos",
    "type": "project",
    "require": {
        "php": "^8.0",
        "google/cloud-vision": "^1.5",
        "vlucas/phpdotenv": "^5.4",
        "firebase/php-jwt": "^6.4",
        "monolog/monolog": "^2.8",
        "intervention/image": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "FoTeam\\": "app/"
        },
        "files": ["includes/functions.php"]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
